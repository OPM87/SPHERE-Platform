    <script type="text/javascript">
        var executeScript = function (Callback)
        {
            // Save On-Load
            var Event = window.onload;
            // Init On-Load
            if (typeof window.onload != 'function') {
                window.onload = function ()
                {
                    executeScript = function (Callback)
                    {
                        Callback();
                    };
                };
                Event = window.onload;
            }
            // Append On-Load
            window.onload = function () {
                if (Event) {
                    Event();
                }
                Callback();
            };
        }
    </script>
